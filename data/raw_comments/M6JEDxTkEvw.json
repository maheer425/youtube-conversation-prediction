        {
            "text": "THEY&#39;RE AS AMERICAN AS", 
            "dur": "1.52", 
            "start": "223.62"
        }, 
        {
            "text": "HOT DOGS AND BASEBALL.", 
            "dur": "2", 
            "start": "225.14"
        }, 
        {
