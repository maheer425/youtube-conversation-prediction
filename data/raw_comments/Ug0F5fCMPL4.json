    "captions": [
        {
            "text": "This is Spudgy", 
            "dur": 0, 
            "start": "0.44"
        }, 
        {
            "text": "Spudgy is a dog", 
            "dur": 0, 
            "start": "3.93"
        }, 
        {
            "text": "Spudgy barks", 
            "dur": 0, 
            "start": "7.14"
        }, 
        {
            "text": "[crickets chirp]", 
            "dur": 0, 
            "start": "8.57"
        }, 
        {
            "text": "I said, Spudgy barks", 
            "dur": 0, 
            "start": "11.35"
        }, 
        {
            "text": "[crickets chirp some more]", 
            "dur": 0, 
            "start": "13.54"
        }, 
        {
            "text": "Come on Spudgy!  Just follow your lines!  It&#39;s written right here in your script!", 
            "dur": 0, 
            "start": "16.51"
