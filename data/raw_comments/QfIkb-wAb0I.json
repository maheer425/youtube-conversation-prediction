    "captions": [
        {
            "text": "Hey, guys.", 
            "dur": "0.86", 
            "start": "4.32"
        }, 
        {
            "text": "I&#39;m Mindy from Cute\nGirls Hairstyles.", 
            "dur": "2.08", 
            "start": "5.18"
        }, 
        {
            "text": "And this morning,\nwe are going to film", 
