        }, 
        {
            "text": "cry...", 
            "dur": "0.43", 
            "start": "119.619"
        }, 
        {
            "text": "VOICE (CONT&#39;D)\nNerds cheer...", 
            "dur": "1.281", 
            "start": "120.049"
        }, 
        {
            "text": "VOICE (CONT&#39;D)\nAnd nerds go...whuh?", 
            "dur": "2.149", 
            "start": "121.33"
        }, 
        {
            "text": "VOICE (CONT&#39;D)\nExplore a brand new Star Trek", 
            "dur": "4.18", 
            "start": "123.479"
