        {
            "text": "[wild cheers and applause]", 
            "dur": "2.334", 
            "start": "251.166"
        }, 
        {
            "text": "- VERY NICE!", 
            "dur": "2", 
            "start": "253.5"
        }, 
        {
            "text": "- ALL RIGHT.", 
            "dur": "1.033", 
            "start": "255.5"
        }, 
        {
            "text": "PRETTY GOOD.\nGOOD JOB, SEAN.", 
            "dur": "1.267", 
            "start": "256.533"
        }, 
        {
            "text": "- WHAT WAS HIS NAME?\nSEAN?", 
